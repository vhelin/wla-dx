; MISC
cd wlalink
smake clean
smake
copy wlalink to /binaries/
cd /

cd wlad
smake clean
smake
copy wlad to /binaries/
cd /

cd wlab
smake clean
smake
copy wlab to /binaries/
cd /

; ARCHIVE
lha a ram:wla.lha binaries/#?
